    "scripts": {
        "build:tailwind": "npx tailwindcss -i ./src/styles/App.css -o ./src/styles/output.css",
        "watch:tailwind": "npx tailwindcss -i ./src/styles/App.css -o ./src/styles/output.css --watch",
        "start:local": "set \"REACT_APP_DEV=local\"&& set \"GENERATE_SOURCEMAP=false\" && npm run build:tailwind & craco start",
        "start:dev": "set \"REACT_APP_DEV=development\"&& set \"GENERATE_SOURCEMAP=false\" && npm run build:tailwind & craco start",
        "start:devios": "REACT_APP_DEV=development GENERATE_SOURCEMAP=false craco start",
        "start:prod": "set \"REACT_APP_DEV=production\"&& set \"GENERATE_SOURCEMAP=false\" && npm run watch:tailwind & craco start",
        "build": "set \"REACT_APP_DEV=production\" && set \"GENERATE_SOURCEMAP=false\" && npm run build:tailwind & craco build",
        "buildios": "REACT_APP_DEV=production GENERATE_SOURCEMAP=false && npm run build:tailwind && craco build",
        "start-dontuse": "react-scripts start",
        "build-dontuse": "react-scripts build",
        "test-dontuse": "react-scripts test"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
